{
    "name": "mikk150/yii2-tagdependency-invalidator",
    "description": "Invalidates Yii2's TagDependency tags on model update/delete and insert",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "tagdependency",
        "cache"
    ],
    "license": "GPL-2.0",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Mikk Tendermann",
            "email": "mikk150@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1.22",
        "codeception/module-asserts": "^1.0.0",
        "yiisoft/yii2-coding-standards": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "mikk150\\tagdependency\\": "src/"
        }
    }
}